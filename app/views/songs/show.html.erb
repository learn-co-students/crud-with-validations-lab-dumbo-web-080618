<h1><%= @song.title %></h1>
<h3><%= @song.release_year %></h3>
<h4>Artist: <%= @song.artist_name %></h4>
<h4>Genre: <%= @song.genre %></h4>


<%= link_to "All Songs", songs_path %><br></br>
<%= link_to "Edit Song", edit_song_path(@song) %><br></br>
<%= button_to("Delete Song", song_path(@song), method: :delete) %><br></br>
